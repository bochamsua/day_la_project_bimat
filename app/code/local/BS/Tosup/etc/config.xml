<config>
    <modules>
        <BS_Tosup>
            <version>1.0.0</version>
        </BS_Tosup>
    </modules>
    <global>
        <resources>
            <bs_tosup_setup>
                <setup>
                    <module>BS_Tosup</module>
                    <class>BS_Tosup_Model_Resource_Setup</class>
                </setup>
            </bs_tosup_setup>
        </resources>
        <blocks>
            <bs_tosup>
                <class>BS_Tosup_Block</class>
            </bs_tosup>
        </blocks>
        <helpers>
            <bs_tosup>
                <class>BS_Tosup_Helper</class>
            </bs_tosup>
        </helpers>
        <models>
            <bs_tosup>
                <class>BS_Tosup_Model</class>
                <resourceModel>bs_tosup_resource</resourceModel>
            </bs_tosup>
            <bs_tosup_resource>
                <class>BS_Tosup_Model_Resource</class>
                <entities>
                    <tosup>
                        <table>bs_tosup_tosup</table>
                    </tosup>
                </entities>
            </bs_tosup_resource>
        </models>
    </global>
    <adminhtml>
        <layout>
            <updates>
                <bs_tosup>
                    <file>bs_tosup.xml</file>
                </bs_tosup>
            </updates>
        </layout>
        <translate>
            <modules>
                <BS_Tosup>
                    <files>
                        <default>BS_Tosup.csv</default>
                    </files>
                </BS_Tosup>
            </modules>
        </translate>
        <global_search>
            <tosup>
                <class>bs_tosup/adminhtml_search_tosup</class>
                <acl>bs_tosup</acl>
            </tosup>
        </global_search>
    </adminhtml>
    <admin>
        <routers>
            <adminhtml>
                <args>
                    <modules>
                        <BS_Tosup before="Mage_Adminhtml">BS_Tosup_Adminhtml</BS_Tosup>
                    </modules>
                </args>
            </adminhtml>
        </routers>
    </admin>
</config>
